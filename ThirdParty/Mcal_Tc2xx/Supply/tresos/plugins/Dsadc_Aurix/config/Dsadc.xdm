<datamodel version="3.0" 
           xmlns="http://www.tresos.de/_projects/DataModel2/08/root.xsd" 
           xmlns:a="http://www.tresos.de/_projects/DataModel2/08/attribute.xsd" 
           xmlns:v="http://www.tresos.de/_projects/DataModel2/06/schema.xsd" 
           xmlns:d="http://www.tresos.de/_projects/DataModel2/06/data.xsd">

  <d:ctr type="AUTOSAR" factory="autosar"  
         xmlns:ad="http://www.tresos.de/_projects/DataModel2/08/admindata.xsd" 
         xmlns:icc="http://www.tresos.de/_projects/DataModel2/08/implconfigclass.xsd"
         xmlns:mt="http://www.tresos.de/_projects/DataModel2/11/multitest.xsd" >
    <d:lst type="TOP-LEVEL-PACKAGES">
      <d:ctr name="AURIX" type="AR-PACKAGE">
        <a:a name="UUID" value="DCE:3ae9f400-6b6b-42ec-8312-f89a60000000"/>
        <d:lst type="ELEMENTS">
          <d:chc name="DsadcParameterDefinition" type="AR-ELEMENT" 
                 value="ECU_PARAMETER_DEFINITION">
            <d:ctr type="AR-ELEMENT">
              <a:a name="DEF" 
                   value="ASPath:/AR_PACKAGE_SCHEMA/ECU_PARAMETER_DEFINITION"/>
              <a:a name="UUID" value="ECUC:4c30de73-30fc-43b3-a1b3-e74210000000"/>
            </d:ctr>
          </d:chc>
          <d:chc name="Dsadc" type="AR-ELEMENT" value="MODULE-DEF">
            <v:ctr type="MODULE-DEF">
              <a:a name="RELEASE" value="asc:4.0"/>
              <a:a name="UUID" value="DCE:d9c9a60e-d7b6-4d89-9688-5de760000000"/>
              <a:a name="DESC" 
                   value="EN: Configuration of the Dsadc (Delta Sigma Analog Digital Conversion) module."/>
              <a:a name="LOWER-MULTIPLICITY" value="0"/>
              <a:a name="UPPER-MULTIPLICITY" value="1"/>
              <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
			  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                <a:a name="LABEL" value="Config Variant"/>
                <a:da name="DEFAULT" value="VariantPostBuildSelectable"/>
                <a:da name="RANGE">
                  <a:v>VariantPostBuildLoadable</a:v>
                  <a:v>VariantPostBuildSelectable</a:v>
                </a:da>
              </v:var>
              
              <v:ctr name="DsadcGeneral" type="IDENTIFIABLE">                
                <a:a name="DESC" 
                     value="EN: This is the general container that contains the post-build selectable configuration parameters"/>                     
                <a:a name="UUID" 
                     value="DCE:dcc39b88-cc2b-4358-ba88-30120722c6ca"/>
                <v:var name="DsadcDevErrorDetect" type="BOOLEAN">
                  <a:a name="DESC" 
                       value="FR: Enables/Disables the Development Error Detection in the Dsadc Driver"/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="UUID" 
                       value="DCE:a4e850e4-be6b-4d72-b360-c13bb0bff7fa"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="TOOLTIP" value="Enables/Disables the Development Error Detection "/>
                </v:var>
                <v:var name="DsadcVersionInfoApi" type="BOOLEAN">
                  <a:a name="DESC" 
                       value="FR: This parameter decides the availability of the API Dsadc_GetVersionInfo in the code"/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="UUID" 
                       value="DCE:9812df82-892b-4abc-99d9-66bfcc6198a1"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="TOOLTIP" value="Enable or disable version info API call "/>
                </v:var>
                <v:var name="DsadcDeInitApi" type="BOOLEAN">
                  <a:a name="DESC" 
                       value="FR: This parameter decides the availability of the API Dsadc_DeInit"/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>                       
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="UUID" 
                       value="DCE:b8ede77d-3270-4da8-b3b5-3bf169de9af0"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="TOOLTIP" value="Enable or disable Deinit API call "/>
                </v:var>
                <v:var name="DsadcPBFixedAddress" type="BOOLEAN">
                  <a:a name="DESC" 
                       value="FR: This parameter decides if PB Fixed Address is ON"/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>                       
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="UUID" 
                       value="DCE:b8ede77d-3270-4da8-b3b5-3bf169de4cc1"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="INVALID" type="XPath">
                  <a:tst expr="(num:i(count(../../DsadcConfigSet/*))) > 1 and . = 'true'"
                   true="Wrong Configuration. When DsadcPBFixedAddress is enabled then only 1 Config set is permitted. Remove the other Config sets."/>
                  </a:da>
                  <a:da name="TOOLTIP" value="Enable or disable PB fixed address "/>
                </v:var>
                <v:var name="DsadcResetSfrAtInit" type="BOOLEAN">
                  <a:a name="DESC" 
                       value="FR: To enable/disable resetting SFR to its reset value in the init function "/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="UUID" 
                       value="DCE:a5d250e4-be6b-4d72-b360-c13bb0bff7fa"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="TOOLTIP" value="Enable/disable resetting SFR "/>
                </v:var>
                <v:var name="DsadcDebugSupport" type="BOOLEAN">
                  <a:a name="DESC" 
                       value="FR: This parameter decides if Dsadc variables are available for debug"/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>                       
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="UUID" 
                       value="DCE:b8ede77d-3270-4da8-b3b5-3bf169de4cc2"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="TOOLTIP" value="Enable or disable usage of debug variables "/>
                </v:var> 
                <v:var name="DsadcEnableLimitCheck" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>"FR: Enable / Disable auxiliary filter chain of DSADC.
                                  true: The Limit checking feature is enabled for Dsadc driver.
                                  false: The Limit checking feature is disabled for Dsadc driver."</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>                       
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="UUID" value="021db8da-97dc-4ad8-a8c3-187626822cc3"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="TOOLTIP" value="Enable or disable usage of auxiliary filter chain "/>
                </v:var>  
                <v:var name="DsadcResolverSupport" type="BOOLEAN">
                  <a:a name="DESC" 
                       value="FR: This parameter decides if Dsadc support resolver feature - Carrier generation, Rectifier and synchronization"/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>                       
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="UUID" 
                       value="DCE:b8ede77d-3270-4da8-b3b5-3bf169deccc2"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="TOOLTIP" value="Enable or disable carrier generation "/>
                </v:var> 
                <v:var name="DsadcInitialConfigEnable" type="BOOLEAN">
                  <a:a name="DESC" 
                       value="FR: This parameter is used to enable configuring of initial global and channel configuration registers.
                                  This parameter is applicable for TC27x DA, TC26x BB, TC29x BB devices. 
                                  User should ensure only these devices are used before enabling this parameter."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>                       
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="UUID" 
                       value="DCE:b8ede77d-3e40-4da8-b3b5-3bf169deccc2"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="WARNING" type="XPath">
                    <a:tst expr="(. = 'true')" true="The parameter DsadcInitialConfigEnable should be set to true, only for devices TC26xBB,TC26xBA,TC29xBA,TC29xBB TC27xDA"/>
                  </a:da>
                  <a:da name="TOOLTIP" value="Enable or disable configuring initial configuration registers."/>
                </v:var>
                <v:var name="DsadcRunningInUser0Mode" type="BOOLEAN">
                <a:a name="TOOLTIP"  value="EN: Pre-processor switch to enable / disable the driver run in User-0 Mode."/>
                <a:a name="DESC"
                       value="If this parameter is set to TRUE, then the driver run in User-0 Mode."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="bfef9f3f-cf0f-4f7f-9f1f-6f1f2fcfff1f"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="DsadcUserModeDeInitApiEnable" type="BOOLEAN">
                  <a:a name="TOOLTIP" value="EN: Pre-processor switch to enable / disable the execution of DeInit API in User-1 Mode."/>                 
                  <a:a name="DESC" 
                       value="Pre-processor switch to enable / disable the execution of DeInit API in User-1 Mode."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="bfef9f3f-bf0f-4f7f-9f1f-6f1fbf3fff1f"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="DsadcUserModeInitApiEnable" type="BOOLEAN">
                  <a:a name="TOOLTIP" value="EN: Pre-processor switch to enable / disable the execution of Init API in User-1 Mode."/>
                  <a:a name="DESC"
                       value="Pre-processor switch to enable / disable the execution of Init API in User-1 Mode."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="bfef9f3f-cf0f-4f7f-bf1f-6f1fbfcf4f1f"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="DsadcUserModeRuntimeApiEnable" type="BOOLEAN">
                  <a:a name="TOOLTIP"
                       value="EN: Pre-processor switch to enable / disable the execution of all APIs other than Init and DeInit in User-1 Mode."/>
                  <a:a name="DESC"
                       value="Pre-processor switch to enable / disable protected register access in all the APIs other than Init/Deinit APIs"/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="bfef9f3f-cf0f-4f7f-9f1f-bf1fbfcfff5f"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
              </v:ctr>  
             
              <v:lst name="DsadcConfigSet" type="MULTIPLE-CONFIGURATION-CONTAINER">
                <a:da name="MIN" value="1"/>
                <v:ctr name="DsadcConfigSet" type="MULTIPLE-CONFIGURATION-CONTAINER">
                  <a:a name="DESC" 
                       value="EN: This is the base container that contains Dsadc module Related Parameters"/>
                  <a:a name="UUID" value="ccda81dc-46cf-4145-858b-7c947884702f"/>
                  <v:ref name="DsadcSystemClockRef" type="REFERENCE">
                  <a:a name="DESC">
                   <a:v>FR: This parameter refers to the system clock configured using the Mcu_InitClock function.
                           This parameter should refer to a valid McuClockSetting Configuration
                           (/AUTOSAR/EcucDefs/Mcu/McuModuleConfiguration/McuClockSettingConfig/McuClockReferencePoint) </a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                      type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                      <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="UUID" value="9ef075e8-51b4-4d4b-bb53-fc516d8f2b23"/>
                  <a:da name="REF"
                        value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Mcu/McuModuleConfiguration/McuClockSettingConfig/McuClockReferencePoint"/>
                  </v:ref>
                  <v:var name="DsadcTrimmingValue" type="INTEGER">
                        <a:a name="DESC" 
                             value="FR: trimming value for dithering function "/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                         <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>                       
                         <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                        </a:a>                       
                        <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                        <a:a name="UUID" 
                             value="DCE:d6ea9dd9-1d6e-4d4b-b3a3-24e9f55c6ccc"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="ENABLE" type="XPath">
                         <a:tst expr="../../../DsadcGeneral/DsadcInitialConfigEnable = 'true'"
                                     true = "true" false = "false"/>
                        </a:da> 
                        <a:da name="INVALID" type="Range">
                          <a:tst expr="&lt;=7"/>
                          <a:tst expr="&gt;=0"/>
                        </a:da>
                        <a:da name="TOOLTIP" value="Define trimming value for dithering function "/>
                      </v:var> 
                      
                      
                       <v:ctr name="DsadcCMHVoltage" type="IDENTIFIABLE">
                        <a:a name="DESC" 
                             value="EN: This container contains parameters to enable and select Vcmh voltage."/>
                        <a:a name="UUID" value="ccda81dc-46cf-4145-858b-7c9478eccccc"/> 
                        <a:da name="ENABLE" type="XPath">
                         <a:tst expr="( (ecu:get('Dsadc.CommonModeHoldVolExits') = 'true')) "/> 
                        </a:da>
                        <v:var name="DsadcCMHVoltageEnableAlways" type="BOOLEAN">
                            <a:a name="DESC"> 
                            <a:v>"FR: If this value is true - Enable common mode hold voltage all the time
                                      If this value is false - enable common mode hold voltage when INxPVCy or INxNVCy is set"</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                 type="IMPLEMENTATIONCONFIGCLASS">
                             <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>                       
                             <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                            </a:a>                 
                            <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                            <a:a name="UUID" 
                                 value="DCE:d4aec598-5cc7-4487-9530-86ee9e8ccc31"/>
                            <a:da name="DEFAULT" value="false"/>
                            <a:da name="TOOLTIP" value="Enable common mode hold voltage"/>
                          </v:var>   
                           <v:var name="DsadcCMHVoltageSelect" type="ENUMERATION">
                             <a:a name="DESC"> 
                            <a:v>"FR: Defines the common mode hold voltage for Dsadc Channel
                                CMHVOLTAGE_A = VDDM div 2.00 (1.65 V for VAREF = 5.0 V)
                                CMHVOLTAGE_B = VDDM / 2.27 (2.2 V for VAREF = 5.0 V)
                                CMHVOLTAGE_C = VDDM / 3.03 (2.5 V for VAREF = 5.0 V)"</a:v>   
                               </a:a>     
                            <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                 type="IMPLEMENTATIONCONFIGCLASS">
                             <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>                       
                             <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                            </a:a> 
                            <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                            <a:a name="UUID" 
                                 value="DCE:d4aec598-5cc7-4487-9530-96ee9e855cc8"/>
                            <a:da name="DEFAULT" value="CMHVOLTAGE_A"/>
                            <a:da name="RANGE">
                            <a:v>CMHVOLTAGE_A</a:v>
                            <a:v>CMHVOLTAGE_B</a:v>
                            <a:v>CMHVOLTAGE_C</a:v>
                            </a:da>         
                          <a:da name="TOOLTIP" value="Select common mode hold voltage"/>                            
                          </v:var>
                          
                    </v:ctr>
                          
                      
                      
                      
                    <v:ctr name="DsadcClockConfiguration" type="IDENTIFIABLE">                      
                      <a:a name="POSTBUILDCHANGEABLE" value="false"/>
                      <a:a name="UUID" 
                           value="DCE:6062ca80-76f1-43eb-8fae-6b10a59e2ccc"/>
                      <a:a name="DESC" 
                           value="EN: This is the container that contains Dsadc Clock Reelated Parameters"/>                           
                       <v:var name="DsadcClockSource" type="ENUMERATION">
                        <a:a name="DESC" 
                             value="FR: Dsadc Clock to be used for internal Modulator"/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                         <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>                       
                         <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                        </a:a>                       
                        <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                        <a:a name="UUID" 
                             value="DCE:213664d2-5a6b-41ca-af7d-cbfa8215fccc"/>
                        <a:da name="DEFAULT" value="CLOCK_FDSD"/>
                        <a:da name="RANGE">
                          <a:v>CLOCK_OFF</a:v>  
                          <a:v>CLOCK_FDSD</a:v> 
                          <a:v>CLOCK_FERAY</a:v>
                          <a:v>CLOCK_FOSC0</a:v> 
                        </a:da>
                        <a:da name="TOOLTIP" value="Select source clock for DSADC"/>
                      </v:var>
                       <v:var name="DsadcPowerSupply" type="ENUMERATION">
                        <a:a name="DESC" 
                             value="FR: Dsadc Low Power Supply select for internal operation"/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                         <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>                       
                         <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                        </a:a>                      
                        <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                        <a:a name="UUID" 
                             value="DCE:213664d2-5a6b-41ca-af7d-cbfa8215fcc1"/>
                        <a:da name="DEFAULT" value="POWERSUPPLY_5V"/>
                        <a:da name="RANGE">
                          <a:v>POWERSUPPLY_5V</a:v>  
                          <a:v>LOWPOWERSUPPLY_3_3V</a:v> 
                        </a:da>
                        <a:da name="TOOLTIP" value="Select power supply "/>
                      </v:var>                      
                      <v:var name="DsadcSleepMode" type="ENUMERATION">
                        <a:a name="DESC" 
                             value="FR: Enables/Disables the Sleep Mode for the DSADC peripheral"/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                         <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>                       
