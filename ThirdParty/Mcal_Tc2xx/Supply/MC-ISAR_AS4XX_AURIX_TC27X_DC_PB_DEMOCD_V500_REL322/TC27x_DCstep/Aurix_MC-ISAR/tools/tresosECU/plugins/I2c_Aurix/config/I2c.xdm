<datamodel version="3.0"
           xmlns="http://www.tresos.de/_projects/DataModel2/08/root.xsd"
           xmlns:a="http://www.tresos.de/_projects/DataModel2/08/attribute.xsd"
           xmlns:v="http://www.tresos.de/_projects/DataModel2/06/schema.xsd"
           xmlns:d="http://www.tresos.de/_projects/DataModel2/06/data.xsd">

  <d:ctr type="AUTOSAR" factory="autosar"
         xmlns:ad="http://www.tresos.de/_projects/DataModel2/08/admindata.xsd"
         xmlns:icc="http://www.tresos.de/_projects/DataModel2/08/implconfigclass.xsd">

    <d:lst type="TOP-LEVEL-PACKAGES">

      <d:ctr name="AURIX" type="AR-PACKAGE">
        <a:a name="UUID" value="9a8b82b6-c3a9-4a4d-b2d3-617b2e1b4a0c"/>
        <d:lst type="ELEMENTS">
          <d:chc name="I2c" type="AR-ELEMENT" value="MODULE-DEF">
            <v:ctr type="MODULE-DEF">
              <a:a name="DESC"
                   value="EN: Configuration of the I2c (Inter-Integrated Circuit) module."/>
              <a:a name="RELEASE" value="asc:4.0"/>
              <a:a name="UUID" value="739a7374-887e-4990-bb70-3ef0ae98eb91"/>
              <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
                <a:a name="LABEL" value="ConfigVariant"/>
                <a:da name="DEFAULT" value="VariantPostBuildSelectable"/>
                <a:da name="RANGE">
                  <a:v>VariantPostBuildLoadable</a:v>
                  <a:v>VariantPostBuildSelectable</a:v>
                </a:da>
                <a:da name="EDITABLE" value="false"/>
              </v:var>

              <v:ctr name="I2cGeneral" type="IDENTIFIABLE">
                <a:a name="DESC"
                     value="EN: General configuration settings for I2C-Handler"/>
                <a:a name="UUID" value="564fe124-75c8-40a1-b55f-e39bf801966c"/>

                <v:var name="I2cDevErrorDetect" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Switches the Development Error Detection and Notification ON or OFF."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="6ef30998-101c-4cfe-a0fb-d8b6a23527ac"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="TOOLTIP" value="Switches the Development Error Detection and Notification ON or OFF"/>
                </v:var>

                <v:var name="I2cVersionInfoApi" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Switches the I2c_GetVersionInfo function ON or OFF."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="7e18c5e2-13bb-4abb-aa16-5fbc825abfc5"/>
                  <a:da name="DEFAULT" value="true"/>
                  <a:da name="TOOLTIP" value="Switches the I2c_GetVersionInfo function ON or OFF"/>
                </v:var>

                <v:var name="I2cHwStatusApi" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Switches the I2c_GetHWUnitStatus function ON or OFF."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="1d02bb89-a7de-4ce0-884c-fb87f9c891b0"/>
                  <a:da name="DEFAULT" value="true"/>
                  <a:da name="TOOLTIP" value="Switches the I2c_GetHWUnitStatus function ON or OFF "/>
                </v:var>

                <v:var name="I2cPBFixedAddress" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Enable/Disable parameter for the selection of fixed adress. ON: fixed address used, OFF: Post build configuration selected."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="7e18c5e2-13bb-4abb-aa16-5fbc825abfc6"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="TOOLTIP" value="Enable/Disable parameter for the selection of fixed adress. ON: fixed address used, OFF: Post build configuration selected."/>
                  <a:da name="INVALID" type="XPath">
                  <a:tst expr="(num:i(count(../../I2cDriver/*))) > 1 and . = 'true'"
                   true="Wrong Configuration. When I2cPBFixedAddress is enabled then only 1 Config set is permitted. Remove the other Config sets."/>
                  </a:da>
                </v:var>

                <v:var name="I2cAsyncReadWrite" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Switches Asynchronous read/write operation ON or OFF."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="d2f53233-0a95-4863-8c21-8c1ea554c81d"/>
                  <a:da name="DEFAULT" value="true"/>
                  <a:da name="TOOLTIP" value="Switches Asynchronous read/write operation ON or OFF"/>
                </v:var>
                <v:var name="I2cResetSfrAtInit" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Switches to enable or disable reset of SFR in Init function."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="d2f53233-0a95-4863-8c21-8c1ea554d90e"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="TOOLTIP" value="Switches to enable or disable reset of SFR in Init function."/>
                </v:var>
                <v:var name="I2cAsyncNotification" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Switches Asynchronous notification ON or OFF."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="d2f54233-0a95-4863-8c21-8c1ea554c81d"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" value="false"/> 
                  <!--<a:da name="EDITABLE" type="XPath">
                  <a:tst expr="../I2cAsyncReadWrite = 'true'"/>
                  </a:da>-->
                  <a:da name="TOOLTIP" value="Switches Asynchronous notification ON or OFF"/>
                </v:var>

                <v:var name="I2cCancelApi" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Switches the I2c_Cancel function ON or OFF."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="d2f55233-0a95-4863-8c21-8c1ea554c81d"/>
                  <a:da name="DEFAULT" value="true"/>
                  <a:da name="EDITABLE" type="XPath">
                  <a:tst expr="../I2cAsyncReadWrite = 'true'"/>
                  </a:da>
                  <a:da name="TOOLTIP" value="Switches the I2c_Cancel function ON or OFF"/>
                </v:var>

                <v:var name="I2cRunningInUser0Mode" type="BOOLEAN">
                <a:a name="TOOLTIP"  value="EN: Pre-processor switch to enable / disable the driver run in User-0 Mode."/>
                <a:a name="DESC"
                       value="If this parameter is set to TRUE, then the driver run in Uer-0 Mode."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="bfef9f3f-cf0f-4f7f-9f1f-6f5f2fcfff1f"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>

                <v:var name="I2cUserModeDeInitApiEnable" type="BOOLEAN">
                  <a:a name="TOOLTIP" value="EN: Pre-processor switch to enable / disable the execution of DeInit API in User-1 Mode."/>                 
                  <a:a name="DESC" 
                       value="Pre-processor switch to enable / disable the execution of DeInit API in User-1 Mode."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="bfef9f3f-bf0f-4f7f-9f1f-6f1f4f3fff1f"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>

                <v:var name="I2cUserModeInitApiEnable" type="BOOLEAN">
                  <a:a name="TOOLTIP" value="EN: Pre-processor switch to enable / disable the execution of Init API in User-1 Mode."/>
                  <a:a name="DESC"
                       value="Pre-processor switch to enable / disable the execution of Init API in User-1 Mode."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="bfef9f3f-cf0f-4f7f-bf1f-6f1fbf3f4f1f"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>

                <v:var name="I2cUserModeRuntimeApiEnable" type="BOOLEAN">
                  <a:a name="TOOLTIP"
                       value="EN: Pre-processor switch to enable / disable the execution of all APIs other than Init and DeInit in User-1 Mode."/>
                  <a:a name="DESC"
                       value="Pre-processor switch to enable / disable protected register access in all the APIs other than Init/Deinit APIs"/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="bfef9f3f-cf0f-4f7f-9f1f-bf1fbfcf1f5f"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
              </v:ctr>

              <v:lst name="I2cHwConfiguration" type="MAP">
                <a:da name="MIN" value="1"/>
                <a:da name="MAX" type="XPath" expr="ecu:get('I2c.NoOfAvailableI2c')"/>
                <v:ctr name="I2cHwConfiguration"
                       type="IDENTIFIABLE">
                <a:a name="DESC"
                     value="Configuration settings I2C hardware-0"/>
                  <a:a name="LONG-NAME"
                       value="I2cHwConfiguration"/>
                  <a:a name="UUID"
                       value="DCE:46354a81-d2f8-6a3d-9a0a-546962bdd907"/>
                  <v:var name="I2cCommunicationType" type="ENUMERATION">
                    <a:a name="DESC"
                         value="EN: This parameter defines the communication type of the I2C peripheral."/>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS"
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                      <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID"
                         value="a64deb81-f92e-4ba5-98b0-c301a8becdd2"/>
                    <a:da name="DEFAULT" value="SYNCHRONOUS"/>
                    <a:da name="RANGE">
                      <a:v>SYNCHRONOUS</a:v>
                      <a:v>ASYNCHRONOUS</a:v>
                    </a:da>
                    <a:da name="ENABLE" type="XPath">
                      <a:tst expr="../../../I2cGeneral/I2cAsyncReadWrite = 'true'"/>
                    </a:da>
                    <a:da name="TOOLTIP" value="This parameter defines the communication type of the I2C peripheral. "/>
                  </v:var>

                  <v:var name="I2cSleepEnable" type="BOOLEAN">
                    <a:a name="DESC"
                         value="EN: To control I2Cx sleep mode"/>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS"
                         type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID"
                         value="20398301-18ac-4596-83ef-b364ac76a8c1"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="TOOLTIP" value="To control I2Cx Hw sleep mode."/>
                  </v:var>

                </v:ctr>
              </v:lst>

              <v:lst name="I2cDriver" type="MULTIPLE-CONFIGURATION-CONTAINER">
                <a:da name="MAX" value="1"/>
                <a:da name="MIN" value="1"/>

                <v:ctr name="I2cDriver" type="MULTIPLE-CONFIGURATION-CONTAINER">
                  <a:a name="DESC"
                       value="Configuration of I2C driver"/>

                  <a:a name="UUID" value="6374e8d2-b2d7-4c08-8a32-b68f54bda4ca"/>

                  <v:ref name="I2cSystemClock" type="REFERENCE">
                    <a:a name="DESC"
                         value="EN: This parameter refer MCU read the System clock configured"/>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS"
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                      <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                    <a:a name="UUID"
                         value="7a12e6a7-f02f-4d95-8b8a-87a36bed8fb9"/>
                    <a:da name="REF"
                          value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Mcu/McuModuleConfiguration/McuClockSettingConfig/McuClockReferencePoint"/>
                      <a:da name="TOOLTIP" value="This parameter refer to the System clock configured in MCU module."/>
                  </v:ref>

                  <v:var name="I2cMaxPacket" type="INTEGER">
                    <a:da name = "ENABLE" value = "false"/>
                    <a:a name="DESC"
                         value="EN: This parameter contains the number of Packets configured. It will be gathered by tools during the configuration stage."/>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS"
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                      <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID"
                         value="ab000ab4-a03e-4365-ab99-a3b38d66f182"/>
                    <a:da name="DEFAULT" value="0"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&lt;=255"/>
                      <a:tst expr="&gt;=0"/>
                    </a:da>
                  </v:var>

                  <v:lst name="I2cPacket" type="MAP">
                    <a:da name="MAX" value="255"/>
                    <a:da name="MIN" value="1"/>
                    <v:ctr name="I2cPacket" type="IDENTIFIABLE">
                      <a:a name="DESC"
                           value="Configuration of I2C packet"/>
                      <a:a name="UUID"
                           value="91a17237-e5c5-4ef6-a712-a4857621814a"/>

                      <v:var name="I2cPacketId" type="INTEGER">
                        <a:a name="DESC"
                             value="EN: This is ID number assigned to I2C channel"/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                        <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                        <a:a name="UUID"
                             value="61ea525c-a919-46b3-834d-bdc06f1ec2ff"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="INVALID" type="Range">
                          <a:tst expr="&lt;=255"/>
                          <a:tst expr="&gt;=0"/>
                        </a:da>
                        <a:da name = "ENABLE" value = "false"/>
                        <a:da name="TOOLTIP" value="Packet Id is generated by the tool"/>
                      </v:var>

                      <v:var name="I2cPacketAddress" type="INTEGER">
                        <a:a name="DESC"
                             value="EN: This parameter contains the address of the Packet."/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                          <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID"
                             value="ea83e38c-1fd0-4493-90a8-f2bdc80c3aac"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="INVALID" type="Range">
                          <a:tst expr="&lt;=128"/>
                          <a:tst expr="&gt;=0"/>
                        </a:da>
                        <a:da name="TOOLTIP" value="This parameter contains the address of the Packet."/>
                      </v:var>

                      <v:var name="I2cPacketLength" type="INTEGER">
                        <a:a name="DESC"
                             value="EN: This parameter contains the length of the Packet.
As I2C is delivered as the demo software, the length of the packet is limited to the number of availbale FIFO stages "/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                          <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID"
                             value="ea84e38c-1fd0-4493-90a8-f2bdc80c3aac"/>
                        <a:da name="DEFAULT" value="8"/>
                        <a:da name="INVALID" type="Range">
                          <a:tst expr="&lt;=8"/>
                          <a:tst expr="&gt;=1"/>
                        </a:da>
                        <a:da name="TOOLTIP" value="This parameter contains the length of the Packet."/>
                      </v:var>

                      <v:ref name="I2cDeviceAssignment" type="REFERENCE">
                        <a:a name="DESC"
                             value="EN: Reference to the external device used by this packet"/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                 type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                            <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                            </a:a>
                        <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                        <a:a name="UUID"
                             value="ac64715b-a773-4a4d-ab8a-eb7eb3b5169b"/>
                        <a:da name="REF"
                              value="ASPathDataOfSchema:/AURIX/I2c/I2cDriver/I2cExternalDevice"/>
                        <a:da name="TOOLTIP" value="Reference to the external device used by this packet."/>
                      </v:ref>

                      <v:var name="I2cPacketEndNotification" type="FUNCTION-NAME">
                        <a:a name="DESC"
                             value="EN: This parameter is a reference to a notification function."/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                          <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID"
                             value="999fcf11-8b4f-4719-abee-2f2d75e8614a"/>
                        <a:da name="DEFAULT" value="NULL_PTR"/>
                        <!--<a:da name="ENABLE" type="XPath">
                          <a:tst expr="../../../../../I2cGeneral/I2cAsyncNotification = 'true'"/>
                        </a:da>-->
                        <a:da name="EDITABLE" value="false"/>                        
                        <a:da name="TOOLTIP" value="This parameter is a reference to a Job notification function."/>
                      </v:var>
                    </v:ctr>
                  </v:lst>

                  <v:lst name="I2cExternalDevice" type="MAP">
                    <a:da name="MIN" value="1"/>
                    <v:ctr name="I2cExternalDevice" type="IDENTIFIABLE">
                      <a:a name="DESC"
                           value="EN: The communication settings of an external device."/>
                      <a:a name="UUID"
                           value="91a17236-e5c5-4ef6-a712-a4857621814a"/>
                           
                      <v:var name="I2cExternalDeviceAddress" type="INTEGER">
