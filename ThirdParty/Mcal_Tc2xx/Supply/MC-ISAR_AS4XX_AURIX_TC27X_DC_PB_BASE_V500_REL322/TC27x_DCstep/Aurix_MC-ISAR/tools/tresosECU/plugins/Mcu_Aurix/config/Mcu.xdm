                <a:da name="DEFAULT" value="VariantPostBuildSelectable"/>
                 <a:da name="RANGE">
                          <a:v>VariantPostBuildSelectable</a:v>
                          <a:v>VariantPostBuildLoadable</a:v>
                        </a:da>
              </v:var>
              <v:ctr name="McuGeneralConfiguration" type="IDENTIFIABLE">
                <a:a name="DESC"
                     value="EN: This container contains the configuration (parameters) of the MCU driver."/>
                <a:a name="UUID" value="6991f177-fe69-4ba3-8e4e-5be27814177b"/>
                <v:var name="McuMainOscillatorFrequency" type="INTEGER">
                  <a:a name="DESC"
                       value="EN: This parameter denotes External crystal frequency value in MHz"/>
                  <a:a name="TOOLTIP"
                       value="This parameter denotes External crystal frequency value in MHz"/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="c6db3011-06c1-4a55-b4dd-4921aed0a185"/>
                  <a:da name="DEFAULT" value="20"/>
                  <a:da name="INVALID" type="Range">
                    <a:tst expr="&lt;=40"/>
                    <a:tst expr="&gt;=8"/>
                  </a:da>
                </v:var>
               <v:var name="McuPllInitDelay" type="INTEGER">
                 <a:a name="DESC"
                      value="EN:McuPllInitDelay configuration paramter defines the delay (in micro seconds) used for PLL initialization "/>
                 <a:a name="TOOLTIP"
                      value="McuPllInitDelay : McuPllInitDelay configuration paramter defines the delay (in micro seconds) used for PLL initialization "/>
                 <a:a name="IMPLEMENTATIONCONFIGCLASS"
                      type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                 <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                 </a:a>
                 <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                 <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                 <a:a name="UUID"
                      value="8c896412-cc63-7895-8cf7-8fcabc698034"/>
                 <a:da name="DEFAULT" value="10"/>

                 <a:da name="INVALID" type="Range">
                   <a:tst expr="&lt;=100"/>
                   <a:tst expr="&gt;=5"/>
                 </a:da>
               </v:var>
                 <v:var name="McuErayPllInitDelay"
                      type="INTEGER">
                 <a:a name="DESC"
                      value="EN: This paramter configures delay (in micro-second) required for Eray PLL initialization"/>
                 <a:a name="TOOLTIP"
                      value="This paramter configures delay (in micro-second) required for Eray PLL initialization"/>
                 <a:a name="IMPLEMENTATIONCONFIGCLASS"
                      type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                 </a:a>
                 <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                 <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                 <a:a name="UUID"
                      value="5dac8944-b238-abc8-8653-bf2685e9ccf7"/>
                 <a:da name="DEFAULT" value="10"/>
                 <a:da name="INVALID" type="Range">
                   <a:tst expr="&lt;=100"/>
                   <a:tst expr="&gt;=5"/>
                 </a:da>
               </v:var>
                <v:var name="McuSystemModeCpuCore" type="ENUMERATION">
                  <a:a name="DESC"
                       value="EN: This parameter will define which core can trigger system modes (sleep / standby). "/>
                  <a:a name="TOOLTIP"
                       value="This parameter will define which core can trigger system modes (sleep / standby). "/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="c6db3011-06c1-4a55-b4dd-4921ffd0a185"/>
                  <a:da name="DEFAULT" value="CPU_CORE_0"/>
                  <a:da name="RANGE" type="XPath" expr="ecu:list('Mcu.SleepCoreEnumValues')"/>                  
                </v:var>
                <v:var name="McuIdleModeCpuCore" type="ENUMERATION">
                  <a:a name="DESC"
                       value="EN: This parameter will define which core can trigger idle mode. "/>
                  <a:a name="TOOLTIP"
                       value="This parameter will define which core can trigger idle mode. "/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="c6db3011-06c1-4a55-b4dd-4921ff123185"/>
                  <a:da name="DEFAULT" value="INDIVIDUAL_CORES"/>
                  <a:da name="RANGE" type="XPath" expr="ecu:list('Mcu.IdleCoreEnumValues')"/>
                </v:var>
                <v:var name="McuDevErrorDetect" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Pre-processor switch for enabling the development error detection and reporting."/>
                  <a:a name="TOOLTIP"
                       value="Pre-processor switch for enabling the development error detection and reporting."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="01eee1fb-1589-455e-9b2b-cead4d920436"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="McuInitClock" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: If this parameter is set to FALSE, the clock initialization has to be disabled from the MCU driver.  If this parameter is set to TRUE, the MCU driver is responsible of the clock initialization."/>
                  <a:a name="TOOLTIP"
                       value="If this parameter is set to FALSE, the clock initialization has to be disabled from the MCU driver.  If this parameter is set to TRUE, the MCU driver is responsible of the clock initialization."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="e15c50b2-14c6-492b-8d8d-c6365dcfd976"/>
                  <a:da name="DEFAULT" value="true"/>
                  <a:da name="EDITABLE" value="false"/>
                </v:var>
                <v:var name="McuNoPll" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: This parameter shall be set True, if the H/W does not have a PLL or the PLL circuitry is enabled after the power on without S/W intervention. In this case MCU_DistributePllClock has to be disabled and MCU_GetPllStatus has to return MCU_PLL_STATUS_UNDEFINED. Otherwise this parameters has to be set False."/>
                  <a:a name="TOOLTIP"
                       value="This parameter shall be set True, if the H/W does not have a PLL or the PLL circuitry is enabled after the power on without S/W intervention. In this case MCU_DistributePllClock has to be disabled and MCU_GetPllStatus has to return MCU_PLL_STATUS_UNDEFINED. Otherwise this parameters has to be set False."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="e15c52b2-14c6-492b-8d8d-c6365dcfd976"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" value="false"/>
                </v:var>
                <v:var name="McuPerformResetApi" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Pre-processor switch to enable / disable the use of the function Mcu_PerformReset()"/>
                  <a:a name="TOOLTIP"
                       value="Pre-processor switch to enable / disable the use of the function Mcu_PerformReset()"/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="2996a537-5ee8-4ab7-87c9-08ed180f8bf3"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="McuVersionInfoApi" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Pre-processor switch to enable / disable the API to read out the modules version information."/>
                  <a:a name="TOOLTIP"
                       value="Pre-processor switch to enable / disable the API to read out the modules version information."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
