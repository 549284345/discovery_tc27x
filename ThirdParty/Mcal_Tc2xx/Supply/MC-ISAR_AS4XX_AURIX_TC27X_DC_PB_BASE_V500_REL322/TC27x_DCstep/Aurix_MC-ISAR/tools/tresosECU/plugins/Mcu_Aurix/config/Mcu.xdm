                  <a:a name="UUID" value="e15c50b2-14c6-492b-8d8d-c6065dcfd976"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="McuFmPllEnable" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Pre-processor switch to enable / disable the modulation of pll frequency"/>
                  <a:a name="TOOLTIP"
                       value="Pre-processor switch to enable / disable the modulation of pll frequency"/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="2496a537-5ef8-487g-51e0-083d1b8e9bf0"/>
                  <a:da name="DEFAULT" value="false"/>
               </v:var>
               <v:var name="McuPBFixedAddress" type="BOOLEAN">
               <a:a name="DESC">
               <a:v>EN: This parameter indicates whether the Mcu configuration to be used is decided at link time or changeable by user at runtime.
                    ON : MCU Configuration to be used is fixed i.e Mcu_ConfigRoot[0]
                    OFF: The user has to choose the MCU configuration to be used and pass the same as a parameter to Mcu_Init</a:v>
                    </a:a>
               <a:a name="TOOLTIP">
               <a:v>This parameter indicates whether the Mcu configuration to be used is decided at link time or changeable by user at runtime.
                    ON : MCU Configuration to be used is fixed i.e Mcu_ConfigRoot[0]
                    OFF: The user has to choose the MCU configuration to be used and pass the same as a parameter to Mcu_Init</a:v>
                    </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="e15c50b2-14c6-492b-8d8d-c6065dcfd576"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="INVALID" type="XPath">
                    <a:tst expr="((num:i(count(../../McuModuleConfiguration/*)) > num:i(1)) and  (. = 'true') )" true = "When McuPBFixedAddress is true, you cannot have multiple configuration instances."/>
                  </a:da>
                </v:var>
                <v:var name="McuGetRamStateApi" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Pre-processor switch to enable/disable the API Mcu_GetRamState."/>
                  <a:a name="TOOLTIP"
                       value="Pre-processor switch to enable/disable the API Mcu_GetRamState."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="e15c50b2-14c6-492b-8d9d-c6065dcfd976"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" value="true"/>
                </v:var>
                <v:var name="McuClearColdResetApi" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Pre-processor switch to enable/disable the API Mcu_ClearColdResetStatus."/>
                  <a:a name="TOOLTIP"
                       value="Pre-processor switch to enable/disable the API Mcu_ClearColdResetStatus."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="123ab8b2-14bc-492b-8d8d-c6065dcfd576"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" value="true"/>
                </v:var>
                <v:var name="McuDeInitApi" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Pre-processor switch to enable / disable the Mcu_DeInit API feature. This parameter control de-initalization for MCU, GTM, DMA and CRC"/>
                  <a:a name="TOOLTIP"
                       value=" Pre-processor switch to enable / disable the Mcu_DeInit API feature. This parameter control de-initalization for MCU, GTM, DMA and CRC"/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="24895337-5ef8-487g-51e0-083d59879bf0"/>
                  <a:da name="DEFAULT" value="false"/>
               </v:var>
                <v:var name="McuResetSfrAtInit" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Pre-processor switch to enable / disable the Mcu SFR resetting at initiliazation for MCU module and GTM module."/>
                  <a:a name="TOOLTIP"
                       value="Pre-processor switch to enable / disable the Mcu SFR resetting at initiliazation for MCU module and GTM module."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="2496a537-5ef8-487g-51e0-0234c58e9bf0"/>
                  <a:da name="DEFAULT" value="false"/>
               </v:var>
               <v:var name="McuRunningInUser0Mode" type="BOOLEAN">
                  <a:a name="DESC"
                       value="If this parameter is set to FALSE, then Protected Mode(user mode) register access will not be done and API is run in Supervisor mode."/>
                  <a:a name="TOOLTIP"
                       value="EN:Pre-processor switch to enable/disable the Normal/Protected mode."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
