                                     type="IDENTIFIABLE">
                                <a:a name="DESC"
                                     value="EN: This container contains the TIM Channel General Configuration"/>
                                <a:a name="UUID"
                                     value="07f27c5d-7c2e-40d3-ae46-40ba97792032"/>
                                <v:var name="TimChannelUsage"
                                       type="ENUMERATION">
                                  <a:a name="DESC">
                                    <a:v>EN: This decides as to if this Channel will be used as a Complex driver or for ICU or will it not be used.
USED_BY_OTHER_DRIVERS_OR_UNUSED: Specifies that this Channel is either unused or configured by other modules(ICU).
USED_BY_GTM_DRIVER: Specifies that this channel will be configured as a complex driver.</a:v>
                                  </a:a>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                       type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                                  </a:a>
                                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                  <a:a name="UUID"
                                       value="03b48085-ef30-424f-992a-b74fd1e993a2"/>
                                  <a:da name="DEFAULT"
                                        value="USED_BY_OTHER_DRIVERS_OR_UNUSED"/>
                                  <a:da name="RANGE">
                                    <a:v>USED_BY_OTHER_DRIVERS_OR_UNUSED</a:v>
                                    <a:v>USED_BY_GTM_DRIVER</a:v>
                                  </a:da>
                                  <a:da name="TOOLTIP" value="To decide if this Channel will be used as a Complex driver or for ICU or will it not be used"/>
                                </v:var>

                                <v:var name="TimChannelEnable"
                                       type="BOOLEAN">
                                  <a:a name="DESC"
                                       value="EN: Enable/Disable the TIM Channel. This sets GTM_TIM0_CHx_CTRL[TIM_EN] for the channel"/>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                       type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                                  </a:a>
                                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                  <a:a name="UUID"
                                       value="8f995d1f-6fbe-441e-a407-5ef109b9aa2e"/>
                                  <a:da name="DEFAULT" value="false"/>
                                  <a:da name="TOOLTIP" value="To enable/disable the Channel"/>
                                  <a:da name="EDITABLE" type="XPath">
                                    <a:tst expr="(../TimChannelUsage = 'USED_BY_GTM_DRIVER')"/>
                                  </a:da>
                                </v:var>

                                <v:var name="TimChannelInputSelect"
                                       type="ENUMERATION">
                                  <a:a name="DESC">
                                    <a:v>EN: This decides the Input for the channel
INPUT_OF_CURRENT_TIM_CHANNEL: Input to the current TIM Channel will be the input assigned to this channel.
INPUT_OF_PREVIOUS_TIM_CHANNEL: Input to the current TIM channel will be the input assigned to the previous channel. Please note, the Port and Pin number applicable in this case is
the configuration in TimChannelPortPinSelect for the previous channel and not the Port and Pin configured for this channel.
INPUT_OF_AUX_IN_TOM: Input to the current TIM channel will be the output of the corresponding TOM channel
INPUT_OF_AUX_IN_ATOM: Input to the current TIM channel will be the output of the corresponding ATOM channel
INPUT_OF_VAL: Input to the current TIM channel will be Fixed value selected by TimChannelValInitSignal.
</a:v>
                                  </a:a>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                       type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                                  </a:a>
                                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                  <a:a name="UUID"
                                       value="44b6b528-6240-4ba6-85c5-9cb69b213850"/>
                                  <a:da name="DEFAULT"
                                        value="INPUT_OF_CURRENT_TIM_CHANNEL"/>
                                  <a:da name="RANGE">
                                    <a:v>INPUT_OF_CURRENT_TIM_CHANNEL</a:v>
                                    <a:v>INPUT_OF_PREVIOUS_TIM_CHANNEL</a:v>
                                    <a:v>INPUT_OF_AUX_IN_TOM</a:v>
                                    <a:v>INPUT_OF_AUX_IN_ATOM</a:v>
                                    <a:v>INPUT_OF_VAL</a:v>
                                  </a:da>
                                  <a:da name="TOOLTIP" value="To decide the Input for the channel"/>
                                </v:var>
                                <v:var name="TimChannelPortPinSelect"
                                       type="ENUMERATION">
                                  <a:a name="DESC"
                                       value="EN: This decides the input port and pin of the TIM Channel. This value is applicable for this channel only if TimChannelInputSelect is configured as 'INPUT_OF_CURRENT_TIM_CHANNEL'"/>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                       type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                                  </a:a>
                                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                  <a:a name="UUID"
                                       value="e48606de-7745-414d-8cc2-cb606e877ec0"/>

                                  <a:da name="DEFAULT" type="XPath">
                                  <a:tst expr="text:split(ecu:get(concat('Gtm.Tim',node:fallback((../../../../@index),'B'),'Channel',node:fallback((../../@index),'MD'))), ', ')[position()-1 = 0]"/>
                                  </a:da>
                                   <a:da name="EDITABLE" type="XPath">
                                    <a:tst expr="(../TimChannelInputSelect = 'INPUT_OF_CURRENT_TIM_CHANNEL')"/>
                                  </a:da>
                                  <a:da name="RANGE" type="XPath" expr="ecu:list(concat('Gtm.Tim',node:fallback((../../../../@index),'B'),'Channel',node:fallback((../../@index),'MD')))"/>
                                  <a:da name="TOOLTIP" value="To decide the input port and pin of the TIM Channel"/>
                                </v:var>
                                 <v:var name="TimChannelValInitSignal"
                                       type="ENUMERATION">
                                  <a:a name="DESC">
                                    <a:v>EN: This decides the input port and pin of the TIM Channel. This value is applicable for this channel only if TimChannelInputSelect is configured as 'INPUT_OF_CURRENT_TIM_CHANNEL'</a:v>
                                    <a:v>VAL_HIGH: Fixed value 1 will be selected</a:v>
                                    <a:v>VAL_LOW: Fixed value 0 will be selected</a:v>
                                  
                                  </a:a>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                       type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                                  </a:a>
                                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                  <a:a name="UUID"
                                       value="a27a9c0f-8c59-45c9-8dac-8a3f65c95e40"/>
                                  <a:da name="DEFAULT" value="VAL_LOW"/>
                                  <a:da name="RANGE">
                                    <a:v>VAL_HIGH</a:v>
                                    <a:v>VAL_LOW</a:v>                                    
                                  </a:da>
                                   <a:da name="EDITABLE" type="XPath">
                                    <a:tst expr="(../TimChannelInputSelect = 'INPUT_OF_VAL')"/>
                                  </a:da>
                                  
                                  <a:da name="TOOLTIP" value="To decide the input port and pin of the TIM Channel"/>
                                </v:var>
                               
                                 <v:var name="TimChannelSignalEdgeSelect"
                                       type="ENUMERATION">
                                  <a:a name="DESC">
                                    <a:v>EN: This decides the signal level to detect for the TIM Channel</a:v>
                                    <a:v>FALLING_EDGE: Falling Edge in the input will be detected</a:v>
                                    <a:v>RISING_EDGE: Rising Edge in the input will be detected</a:v>
                                    <a:v>BOTH_EDGES: Both Edges in the input will be detected</a:v>
                                  </a:a>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                       type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                                  </a:a>
                                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                  <a:a name="UUID"
                                       value="570e9e89-6b4f-42bd-ac8d-406d5e950c7d"/>
                                  <a:da name="DEFAULT" value="FALLING_EDGE"/>
                                  <a:da name="RANGE">
                                    <a:v>FALLING_EDGE</a:v>
                                    <a:v>RISING_EDGE</a:v>
                                    <a:v>BOTH_EDGES</a:v>
                                  </a:da>
                                  <a:da name="TOOLTIP" value="To decide the signal level to detect for the TIM Channel"/>
                                  <a:da name="EDITABLE" type="XPath">
                                    <a:tst expr="(../TimChannelUsage = 'USED_BY_GTM_DRIVER')"/>
                                  </a:da>
                                  <a:da name="INVALID" type="XPath">
                                    <a:tst expr="((. = 'BOTH_EDGES') and (((../../../../@index = '0') and (../TimChannelModeSelect != 'INPUT_EVENT_MODE_TIEM') and (../TimChannelModeSelect != 'INPUT_PRESCALER_MODE_TIPM')) or  ((../../../../@index != '0') and (../TimChannelModeSelect != 'INPUT_EVENT_MODE_TIEM')))) " true = "Both Edges is supported only in TIEM and TIPM modes in case of TIM0 and only in TIEM mode is case of other TIM modules"/>
                                  </a:da>
                                </v:var>
                                <v:var name="TimChannelClockSelect"
                                       type="ENUMERATION">
                                  <a:a name="DESC">
                                    <a:v>EN: This decides the Clock Source for TIM Channel</a:v>
                                    <a:v>GTM_CONFIGURABLE_CLOCK_0: Configurable Clock 0 will be supplied to this channel</a:v>
                                    <a:v>GTM_CONFIGURABLE_CLOCK_1: Configurable Clock 1 will be supplied to this channel</a:v>
                                    <a:v>GTM_CONFIGURABLE_CLOCK_2: Configurable Clock 2 will be supplied to this channel</a:v>
                                    <a:v>GTM_CONFIGURABLE_CLOCK_3: Configurable Clock 3 will be supplied to this channel</a:v>
                                    <a:v>GTM_CONFIGURABLE_CLOCK_4: Configurable Clock 4 will be supplied to this channel</a:v>
                                    <a:v>GTM_CONFIGURABLE_CLOCK_5: Configurable Clock 5 will be supplied to this channel</a:v>
                                    <a:v>GTM_CONFIGURABLE_CLOCK_6: Configurable Clock 6 will be supplied to this channel</a:v>
                                    <a:v>GTM_CONFIGURABLE_CLOCK_7: Configurable Clock 7 will be supplied to this channel</a:v>
                                  </a:a>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                       type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                                  </a:a>
                                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                  <a:a name="UUID"
                                       value="e0e8a863-d29f-42c9-b32f-be917ae6b680"/>
                                  <a:da name="DEFAULT"
                                        value="GTM_CONFIGURABLE_CLOCK_0"/>
                                  <a:da name="RANGE">
                                    <a:v>GTM_CONFIGURABLE_CLOCK_0</a:v>
                                    <a:v>GTM_CONFIGURABLE_CLOCK_1</a:v>
                                    <a:v>GTM_CONFIGURABLE_CLOCK_2</a:v>
                                    <a:v>GTM_CONFIGURABLE_CLOCK_3</a:v>
                                    <a:v>GTM_CONFIGURABLE_CLOCK_4</a:v>
                                    <a:v>GTM_CONFIGURABLE_CLOCK_5</a:v>
                                    <a:v>GTM_CONFIGURABLE_CLOCK_6</a:v>
                                    <a:v>GTM_CONFIGURABLE_CLOCK_7</a:v>
                                  </a:da>
                                  <a:da name="TOOLTIP" value="To decide the Clock Source for TIM Channel"/>
                                </v:var>
                                <v:var name="TimChannelGpr0InputSelect"
                                       type="ENUMERATION">
                                  <a:a name="DESC">
                                    <a:v>EN: This decides the value captured in GPR0 register of TIM Channel.</a:v>
                                    <a:v>TIMEBASE_TBU_TS0: TBU_TS0 will be captured in GPR0</a:v>
                                    <a:v>TIMEBASE_TBU_TS1: TBU_TS1 will be captured in GPR0</a:v>
                                    <a:v>TIMEBASE_TBU_TS2: TBU_TS2 will be captured in GPR0</a:v>
                                    <a:v>CNT_VALUE: CNT value will be captured in GPR0</a:v>
                                    <a:v>ECNT_VALUE: ECNT value will be captured in GPR0</a:v>
                                  
                                  </a:a>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                       type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                                  </a:a>
                                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                  <a:a name="UUID"
                                       value="f11c7a62-fff8-4374-8a96-ff336d21ecf8"/>
                                  <a:da name="DEFAULT" value="TIMEBASE_TBU_TS0"/>
                                  <a:da name="RANGE">
                                    <a:v>TIMEBASE_TBU_TS0</a:v>
                                    <a:v>TIMEBASE_TBU_TS1</a:v>
                                    <a:v>TIMEBASE_TBU_TS2</a:v>
                                    <a:v>CNT_VALUE</a:v>
                                    <a:v>ECNT_VALUE</a:v>
                                  </a:da>
                                  <a:da name="TOOLTIP" value="To decide the value captured in GPR0 register of TIM Channel"/>
                                  <a:da name="INVALID" type="XPath">
                                    <a:tst expr="((../TimChannelUsage = 'USED_BY_OTHER_DRIVERS_OR_UNUSED') and (. = 'CNT_VALUE')) " true = "Only Timebase Values can be captured when used for ICU (USED_BY_OTHER_DRIVERS_OR_UNUSED). CNT value cannot be captured"/>
                                  </a:da>
                                </v:var>
                                <v:var name="TimChannelGpr1InputSelect"
                                       type="ENUMERATION">
                                  <a:a name="DESC">
                                    <a:v>EN: This decides the value captured in GPR1 register of TIM Channel</a:v>
                                    <a:v>TIMEBASE_TBU_TS0: TBU_TS0 will be captured in GPR1</a:v>
                                    <a:v>TIMEBASE_TBU_TS1: TBU_TS1 will be captured in GPR1</a:v>
                                    <a:v>TIMEBASE_TBU_TS2: TBU_TS2 will be captured in GPR1</a:v>
                                    <a:v>CNTS_VALUE: CNT Value will be captured in GPR1</a:v>
                                    <a:v>ECNT_VALUE: CNT Value will be captured in GPR1</a:v>
                                  
                                  </a:a>

                                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                       type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                                  </a:a>
                                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                  <a:a name="UUID"
                                       value="6c7ae929-6886-4445-9739-87ab3ced6b7d"/>
                                  <a:da name="DEFAULT" value="TIMEBASE_TBU_TS0"/>
                                  <a:da name="RANGE">
                                    <a:v>TIMEBASE_TBU_TS0</a:v>
                                    <a:v>TIMEBASE_TBU_TS1</a:v>
                                    <a:v>TIMEBASE_TBU_TS2</a:v>
                                    <a:v>CNTS_VALUE</a:v>
                                    <a:v>ECNT_VALUE</a:v>
                                  </a:da>
                                  <a:da name="TOOLTIP" value="To decide the value captured in GPR1 register of TIM Channel"/>
                                  <a:da name="EDITABLE" type="XPath">
                                    <a:tst expr="(../TimChannelUsage = 'USED_BY_GTM_DRIVER')"/>
                                  </a:da>
                                </v:var>
                                <v:var name="TimChannelCntsInputSelect"
                                       type="ENUMERATION">
                                  <a:a name="DESC">
                                    <a:v>EN: This decides the value captured in CNTS register of TIM Channel. This is not applicable in TIPM and TBCM mode</a:v>
                                    <a:v>CNT_VALUE: CNT Value will be captured in CNTS</a:v>
                                    <a:v>TIMEBASE_TBU_TS0: TBU_TS0 will be captured in CNTS</a:v>
                                  </a:a>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                       type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                                  </a:a>
                                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                  <a:a name="UUID"
                                       value="3d3dae92-f1ea-4599-a136-cacc6917a7a3"/>
                                  <a:da name="DEFAULT" value="CNT_VALUE"/>
                                  <a:da name="RANGE">
                                    <a:v>CNT_VALUE</a:v>
                                    <a:v>TIMEBASE_TBU_TS0</a:v>
                                  </a:da>
                                  <a:da name="TOOLTIP" value="To decide the value captured in CNTS register of TIM Channel"/>
                                  <a:da name="EDITABLE" type="XPath">
                                    <a:tst expr="(../TimChannelUsage = 'USED_BY_GTM_DRIVER') and (../TimChannelModeSelect != 'BIT_COMPRESSION_MODE_TBCM') and (../TimChannelModeSelect != 'INPUT_PRESCALER_MODE_TIPM')"/>
                                  </a:da>
                                </v:var>
                                <v:var name="TimChannelCntsValue"
                                       type="INTEGER">
                                  <a:a name="DESC"
                                       value="EN: This parameter specifies the CNTS Value. This is available only in case of TIPM and TBCM modes"/>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                       type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                                  </a:a>
                                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                  <a:a name="UUID"
                                       value="7da01df6-8cdb-4755-876c-6f990019d5b7"/>
                                  <a:da name="DEFAULT" value="0"/>
                                  <a:da name="TOOLTIP" value="To specify the CNTS Value"/>
                                  <a:da name="EDITABLE" type="XPath">
                                    <a:tst expr="(../TimChannelUsage = 'USED_BY_GTM_DRIVER') and ((../TimChannelModeSelect = 'BIT_COMPRESSION_MODE_TBCM') or (../TimChannelModeSelect = 'INPUT_PRESCALER_MODE_TIPM'))"/>
                                  </a:da>
                                <a:da name="INVALID" type="Multi">
                                <mt:range>
                                <mt:tst expr="&gt;=0" true="valid range is 0 to 16777215"/>
                                <mt:tst expr="&lt;=16777215" true="valid range is 0 to 16777215"/>
                                </mt:range>
                                </a:da>                                   
                                </v:var>
                                <v:var name="TimChannelTbu0Ts0BitsSelect"
                                       type="ENUMERATION">
                                  <a:a name="DESC">
                                    <a:v>EN: This decides the resolution of TBU_TS0 that will be captured in GPR0 and GPR1 register of TIM Channel.</a:v>
                                    <a:v>BITS_0_TO_23: Lower 24 bits will be captured in GPR0/GPR1</a:v>
                                    <a:v>BITS_3_TO_26: upper 24 bits will be captured in GPR0/GPR1</a:v>                                  
                                  </a:a>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                       type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                                  </a:a>
                                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                  <a:a name="UUID"
                                       value="b209c80f-87dd-422e-8f00-fa776e66e9ea"/>
                                  <a:da name="DEFAULT" value="BITS_0_TO_23"/>
                                  <a:da name="RANGE">
                                    <a:v>BITS_0_TO_23</a:v>
                                    <a:v>BITS_3_TO_26</a:v>
                                  </a:da>
                                  <a:da name="TOOLTIP" value="To decide the resolution of TBU_TS0 that will be captured in GPR0 and GPR1 register of TIM Channel"/>
                                  <a:da name="EDITABLE" type="XPath">
                                    <a:tst expr="(../../../../@index = '0') and ((../TimChannelGpr0InputSelect = 'TIMEBASE_TBU_TS0') or (((../TimChannelUsage = 'USED_BY_GTM_DRIVER')) and (../TimChannelGpr1InputSelect = 'TIMEBASE_TBU_TS0')))"/>
                                  </a:da>
                                </v:var>
                                <v:var name="TimChannelModeSelect"
                                       type="ENUMERATION">
                                  <a:a name="DESC">
                                    <a:v>EN: This decides the Operating Mode for the TIM Channel</a:v>
                                    <a:v>PWM_MEASUREMENT_MODE_TPWM: TPWM mode is chosen</a:v>
                                    <a:v>PULSE_INTEGRATION_MODE_TPIM: TPIM mode is chosen</a:v>
                                    <a:v>INPUT_EVENT_MODE_TIEM: TIEM mode is chosen</a:v>
                                    <a:v>INPUT_PRESCALER_MODE_TIPM: TIPM mode is chosen</a:v>
                                    <a:v>BIT_COMPRESSION_MODE_TBCM: TBCM mode is chosen</a:v>
                                  </a:a>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                       type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                                  </a:a>
                                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                  <a:a name="UUID"
                                       value="c03bd047-4062-4085-baee-b55acdfce941"/>
                                  <a:da name="DEFAULT"
                                        value="PWM_MEASUREMENT_MODE_TPWM"/>
                                  <a:da name="RANGE">
                                    <a:v>PWM_MEASUREMENT_MODE_TPWM</a:v>
                                    <a:v>PULSE_INTEGRATION_MODE_TPIM</a:v>
                                    <a:v>INPUT_EVENT_MODE_TIEM</a:v>
                                    <a:v>INPUT_PRESCALER_MODE_TIPM</a:v>
                                    <a:v>BIT_COMPRESSION_MODE_TBCM</a:v>
                                  </a:da>
                                  <a:da name="TOOLTIP" value="To decide the Operating Mode for the TIM Channel"/>
                                  <a:da name="EDITABLE" type="XPath">
                                    <a:tst expr="(../TimChannelUsage = 'USED_BY_GTM_DRIVER')"/>
                                  </a:da>
                                  <a:da name="INVALID" type="XPath">
                                    <a:tst expr="(. = 'BIT_COMPRESSION_MODE_TBCM') and (../../@index != '0')" true = "TBCM mode is applicable only for TIM Channel 0"/>
                                  </a:da>
                                </v:var>
                                <v:var name="TimChannelOneShotMode"
                                       type="BOOLEAN">
                                  <a:a name="DESC"
                                       value="EN: Enables One Shot Mode for the Channel."/>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                       type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                                  </a:a>
                                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                  <a:a name="UUID"
                                       value="28f48546-4ce7-4486-b2e0-2886b1afa9f1"/>
                                  <a:da name="DEFAULT" value="false"/>
                                  <a:da name="TOOLTIP" value="To enable One Shot Mode for the Channel"/>
                                  <a:da name="EDITABLE" type="XPath">
                                    <a:tst expr="(../TimChannelUsage = 'USED_BY_GTM_DRIVER')"/>
                                  </a:da>
                                </v:var>                                
                                <v:var name="TimChannelAruEnable"
                                       type="BOOLEAN">
                                  <a:a name="DESC"
