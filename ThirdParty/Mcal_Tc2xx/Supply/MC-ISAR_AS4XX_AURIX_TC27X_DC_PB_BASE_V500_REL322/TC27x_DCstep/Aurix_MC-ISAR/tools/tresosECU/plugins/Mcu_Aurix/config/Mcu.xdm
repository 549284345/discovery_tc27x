                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID"
                               value="8f883293-79c9-410a-9bde-90ce47513eb3"/>
                          <a:da name="DEFAULT" value="1"/>
                          <a:da name="INVALID" type="Multi">
                          <mt:range>
                          <mt:tst expr="&gt;=0" true="valid range is 0 to 16777215"/>
                          <mt:tst expr="&lt;=16777215" true="valid range is 0 to 16777215"/>
                          </mt:range>
                          </a:da>                           
                          <a:da name="TOOLTIP" value="Decides the Numerator value for the GTM Global clock Divider"/>
                        </v:var>
                        <v:var name="GtmGlobalClkDivDenominator" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>EN: This parameter specifies the Denominator value for the Global Clock Divider.
This modifies GTM_CMU_GCLK_DEN[GCLK_DEN]</a:v>
                          </a:a>
