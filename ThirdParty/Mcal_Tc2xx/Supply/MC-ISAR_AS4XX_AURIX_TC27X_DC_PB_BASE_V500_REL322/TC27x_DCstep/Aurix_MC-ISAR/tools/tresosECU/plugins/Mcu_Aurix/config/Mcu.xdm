                               value="47f1b4a5-3c41-46b6-958e-1cbe6c63ca98"/>
                          <a:da name="DEFAULT" value="GTM_SLEEP_ENABLE"/>
                          <a:da name="RANGE">
                            <a:v>GTM_SLEEP_DISABLE</a:v>
                            <a:v>GTM_SLEEP_ENABLE</a:v>
                          </a:da>
                          <a:da name="TOOLTIP" value="To Enable/Disable the GTM Sleep Mode"/>
                        </v:var>
                        <v:var name="GtmGlobalClkDivNumerator" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>EN: This parameter specifies the Numerator value for the Global Clock Divider.
This modifies GTM_CMU_GCLK_NUM[GCLK_NUM]</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS"
                               type="IMPLEMENTATIONCONFIGCLASS">
